class Vector
{
	__init__(x, y)
	{
		self.x = x;
		self.y = y;
	}

	print()
	{
		print(str(self.x) + ", " + str(self.y));
	}

	__add__(x)
	{
		return Vector(self.x + x.x, self.y + x.y);
	}
}


func main()
{
	var vec1 = Vector(0, 2);
	var vec2 = Vector(3, 4);
	var vec3 = Vector(5, 4);
	var other = vec1 + vec2 + vec3;
	other.print();
}

func println(s)
{
	print(str(s) + "\n");
}
