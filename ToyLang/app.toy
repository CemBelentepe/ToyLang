class Vector
{
	__init__(x, y)
	{
		self.x = x;
		self.y = y;
	}

	__imul__(k)
	{
		self.x *= k;
		self.y *= k;
		return self;
	}

	println()
	{
		println(str(self.x) + ", " + str(self.y));
	}
}


func main()
{
	var arr = Array();
	for(var i = 0; i < 10; i+=1)
		arr.push(Vector(i, i + 1));
	for(var i = 0; i < 10; i+=1)
		arr[i] *= 2;
		
	for(var i = 0; i < 10; i+=1)
	{
		arr[i].println();
	}
}

func println(s)
{
	print(str(s) + "\n");
}
